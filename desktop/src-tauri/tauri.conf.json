{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "FGBrowser",
  "version": "0.1.0",
  "identifier": "com.pc.fgbrowser",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build",
    "runner": "cargo"
  },
  "app": {
    "windows": [
      {
        "title": "FGBrowser",
        "width": 1230,
        "height": 800,
        "minWidth": 1000,
        "minHeight": 600,
        "zoomHotkeysEnabled": true
      }
    ],
    "security": {
      "csp": null,
      "assetProtocol": {
        "enable": true,
        "scope": [
          "$APPCACHE/**"
        ]
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "externalBin": [],
    "macOS": {
      "signingIdentity": null,
      "entitlements": null
    }
  }
}